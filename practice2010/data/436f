	var smf_template_body_edit = '<div id="error_box" style="padding: 4px; color: red;"></div><textarea class="editor" name="message" rows="12" style="width: 94%; margin-bottom: 10px;">%body%</textarea><br /><input type="hidden" name="sc" value="176acd56b2db5f4323ef3883b23ce443" /><input type="hidden" name="topic" value="436" /><input type="hidden" name="msg" value="%msg_id%" /><div style="text-align: center;"><input type="submit" name="post" value="Сохранить" onclick="return modify_save(\'176acd56b2db5f4323ef3883b23ce443\');" accesskey="s" />&nbsp;&nbsp;<input type="submit" name="cancel" value="Отменить" onclick="return modify_cancel();" /></div>';

	// And this is the replacement for the subject.
	var smf_template_subject_edit = '<input type="text" name="subject" value="%subject%" size="60" style="width: 99%;"  maxlength="80" />';

	// Restore the message to this after editing.
	var smf_template_body_normal = '%body%';
	var smf_template_subject_normal = '<a href="http://forum.ubuntu.ru/index.php?topic=436.msg%msg_id%#msg%msg_id%">%subject%</a>';
	var smf_template_top_subject = "Тема: %subject% &nbsp;(Прочитано 12352 раз)"

	if (window.XMLHttpRequest)
		showModifyButtons();
// ]]></script>
