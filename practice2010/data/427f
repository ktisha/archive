	var smf_template_body_edit = '<div id="error_box" style="padding: 4px; color: red;"></div><textarea class="editor" name="message" rows="12" style="width: 94%; margin-bottom: 10px;">%body%</textarea><br /><input type="hidden" name="sc" value="b8c7ad2f4330cc3d475b7090239bac30" /><input type="hidden" name="topic" value="427" /><input type="hidden" name="msg" value="%msg_id%" /><div style="text-align: center;"><input type="submit" name="post" value="Сохранить" onclick="return modify_save(\'b8c7ad2f4330cc3d475b7090239bac30\');" accesskey="s" />&nbsp;&nbsp;<input type="submit" name="cancel" value="Отменить" onclick="return modify_cancel();" /></div>';

	// And this is the replacement for the subject.
	var smf_template_subject_edit = '<input type="text" name="subject" value="%subject%" size="60" style="width: 99%;"  maxlength="80" />';

	// Restore the message to this after editing.
	var smf_template_body_normal = '%body%';
	var smf_template_subject_normal = '<a href="http://forum.ubuntu.ru/index.php?topic=427.msg%msg_id%#msg%msg_id%">%subject%</a>';
	var smf_template_top_subject = "Тема: %subject% &nbsp;(Прочитано 120380 раз)"

	if (window.XMLHttpRequest)
		showModifyButtons();
// ]]></script>
